{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "OriginalHost": "true",
    "Routes": {
      "WebApp": {
        "ClusterId": "qweree.webapp",
        "Match": {
          "Path": "/{*any}"
        }
      },
      "AuthenticationService": {
        "ClusterId": "qweree.authentication",
        "Match": {
          "Path": "/auth/{*any}"
        }
      },
      "CdnService": {
        "ClusterId": "qweree.cdn",
        "Match": {
          "Path": "/cdn/{*any}"
        }
      },
      "PiccService": {
        "ClusterId": "qweree.picc",
        "Match": {
          "Path": "/picc/{*any}"
        }
      }
    },
    "Clusters": {
      "qweree.authentication": {
        "Destinations": {
          "default": {
            "Address": "http://localhost/auth/"
          }
        }
      },
      "qweree.cdn": {
        "Destinations": {
          "default": {
            "Address": "http://localhost/cdn/"
          }
        }
      },
      "qweree.picc": {
        "Destinations": {
          "default": {
            "Address": "http://localhost/picc/"
          }
        }
      },
      "qweree.webapp": {
        "Destinations": {
          "default": {
            "Address": "http://localhost/"
          }
        }
      }
    }
  },
  "Qweree": {
    "Origin": "http://localhost",
    "ClientId": "gateway-cli",
    "ClientSecret": "password",
    "SessionStorage": "/data/session",
    "AuthUri": "http://localhost/auth/",
    "CdnUri": "http://localhost/cdn/api/v1/storage/"
  }
}
