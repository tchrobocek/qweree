{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Urls": "http://*:5000;https://*:5001",
  "Origin": "https://localhost:5001",
  "SessionStorage": "./data/session",
  "AuthUri": "https://localhost/auth/",
  "ReverseProxy": {
    "Routes": {
      "AuthenticationService" : {
        "ClusterId": "qweree.authentication",
        "Match": {
          "Path": "/auth/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/auth"
          }, {
            "ResponseHeader": "Access-Control-Allow-Credentials",
            "Set": "true",
            "When": "Always"
          },{
            "ResponseHeader": "Access-Control-Allow-Origin",
            "Set": "https://localhost:5001",
            "When": "Always"
          }]
      },
      "CdnService" : {
        "ClusterId": "qweree.cdn",
        "Match": {
          "Path": "/cdn/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/cdn"
          }, {
            "ResponseHeader": "Access-Control-Allow-Credentials",
            "Set": "true",
            "When": "Always"
          },{
            "ResponseHeader": "Access-Control-Allow-Origin",
            "Set": "https://localhost:5001",
            "When": "Always"
          }]
      },
      "PiccService" : {
        "ClusterId": "qweree.picc",
        "Match": {
          "Path": "/picc/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/picc"
          }, {
            "ResponseHeader": "Access-Control-Allow-Credentials",
            "Set": "true",
            "When": "Always"
          },{
            "ResponseHeader": "Access-Control-Allow-Origin",
            "Set": "https://localhost:5001",
            "When": "Always"
          }]
      }
    },
    "Clusters": {
      "qweree.authentication": {
        "Destinations": {
          "localhost": {
            "Address": "https://localhost/auth/"
          }
        }
      },
      "qweree.cdn": {
        "Destinations": {
          "localhost": {
            "Address": "https://localhost/cdn/"
          }
        }
      },
      "qweree.picc": {
        "Destinations": {
          "localhost": {
            "Address": "https://localhost/picc/"
          }
        }
      },
      "qweree.webapp": {
        "Destinations": {
          "default": {
            "Address": "https://localhost/"
          }
        }
      }
    }
  }
}
